<p id="notice"><%= notice %></p>

<h1>Lists</h1>

<table class='table'>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= link_to list.name, list_path(list) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'New list', new_list_path %>
